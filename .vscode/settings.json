{
    "editor.formatOnSave": true,
    "prettier.jsxSingleQuote": false,
    "prettier.singleQuote": true,
    "javascript.preferences.quoteStyle": "auto",
    "typescript.preferences.quoteStyle": "auto",
    "html.completion.attributeDefaultValue": "doublequotes",
    "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
    "prettier.semi": false,
    "typescriptHero.imports.insertSemicolons": false,
    "csscomb.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.trimTrailingWhitespace": true,
    "indentRainbow.colorOnWhiteSpaceOnly": false,
    "indentRainbow.lightIndicatorStyleLineWidth": 2,
    "svgeditor.indentSize": 2,
    "editor.autoIndent": "full",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "files.autoSave": "off",
    "[javascript]": {
        "editor.defaultFormatter": "bysabi.prettier-vscode-standard"
    },
    "todo-tree.tree.autoRefresh": false,
    "[css]": {
        "editor.defaultFormatter": "SimonSiefke.prettier-vscode"
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#083144",
        "titleBar.activeBackground": "#0B455F",
        "titleBar.activeForeground": "#F5FBFE"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true
    }
}
